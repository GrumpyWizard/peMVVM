<Window x:Class="peMVVM.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Project Euler" Height="354" Width="521">
  <Grid Margin="0,0,2,0">
    <TextBox HorizontalAlignment="Left" Height="23" Margin="142,22,-23,0" TextWrapping="Wrap" Text="{Binding ProblemId}" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.192,-0.478"/>
    <Button Content="Go" HorizontalAlignment="Left" Margin="275,22,-97,0" VerticalAlignment="Top" Width="61" Command="{Binding Path=GoCommand}"/>
    <TextBlock HorizontalAlignment="Left" Margin="142,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="206" Width="194" Text="{Binding Path=ProblemSolution}"/>
  </Grid>
</Window>
